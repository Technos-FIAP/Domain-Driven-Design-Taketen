package br.com.taketen.beans;

import javax.xml.bind.annotation.XmlRootElement;

@XmlRootElement
public class Usuario {

	private int idUsuario;
	private String numeroUsuario, descricaoEmail, descricaoSenha, dataNascimento;
	private double numeroAltura, numeroPeso;
	private String descricaoSexo;
	private int idEquipe;
	
	public Usuario() {
		super();
	}

	public Usuario(int idUsuario, String numeroUsuario, String descricaoEmail, String descricaoSenha,
			String dataNascimento, double numeroAltura, double numeroPeso, String descricaoSexo, int idEquipe) {
		super();
		this.idUsuario = idUsuario;
		this.numeroUsuario = numeroUsuario;
		this.descricaoEmail = descricaoEmail;
		this.descricaoSenha = descricaoSenha;
		this.dataNascimento = dataNascimento;
		this.numeroAltura = numeroAltura;
		this.numeroPeso = numeroPeso;
		this.descricaoSexo = descricaoSexo;
		this.idEquipe = idEquipe;
	}

	public int getIdUsuario() {
		return idUsuario;
	}

	public String getNumeroUsuario() {
		return numeroUsuario;
	}

	public String getDescricaoEmail() {
		return descricaoEmail;
	}

	public String getDescricaoSenha() {
		return descricaoSenha;
	}

	public String getDataNascimento() {
		return dataNascimento;
	}

	public double getNumeroAltura() {
		return numeroAltura;
	}

	public double getNumeroPeso() {
		return numeroPeso;
	}

	public String getDescricaoSexo() {
		return descricaoSexo;
	}

	public int getIdEquipe() {
		return idEquipe;
	}

	public void setIdUsuario(int idUsuario) {
		this.idUsuario = idUsuario;
	}

	public void setNumeroUsuario(String numeroUsuario) {
		this.numeroUsuario = numeroUsuario;
	}

	public void setDescricaoEmail(String descricaoEmail) {
		this.descricaoEmail = descricaoEmail;
	}

	public void setDescricaoSenha(String descricaoSenha) {
		this.descricaoSenha = descricaoSenha;
	}

	public void setDataNascimento(String dataNascimento) {
		this.dataNascimento = dataNascimento;
	}

	public void setNumeroAltura(double numeroAltura) {
		this.numeroAltura = numeroAltura;
	}

	public void setNumeroPeso(double numeroPeso) {
		this.numeroPeso = numeroPeso;
	}

	public void setDescricaoGenero(String descricaoGenero) {
		this.descricaoGenero = descricaoGenero;
	}

	public void setIdEquipe(int idEquipe) {
		this.idEquipe = idEquipe;
	}
	

}
